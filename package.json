{
    "name": "peggy-to-ts",
    "version": "2.0.0",
    "description": "Generate Typescript types from PEGjs/Peggy grammars",
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./*js": "./dist/*js",
        "./*": {
            "import": "./dist/*.js",
            "require": "./dist/*.cjs"
        }
    },
    "scripts": {
        "build": "npm run compile",
        "compile": "tsc -b tsconfig.json & node build.js",
        "test": "vitest",
        "install": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/siefkenj/peggy-to-ts.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/siefkenj/peggy-to-ts/issues"
    },
    "homepage": "https://github.com/siefkenj/peggy-to-ts#readme",
    "devDependencies": {
        "@types/node": "^20.5.9",
        "jest": "^29.6.4",
        "prettier-plugin-pegjs": "^2.0.2",
        "vitest": "^2.1.6"
    },
    "dependencies": {
        "peggy": "^4.2.0",
        "prettier": "^3.0.3",
        "ts-morph": "^19.0.0",
        "typescript": "^5.2.2"
    }
}
